{
  "name": "solid-marked",
  "version": "0.3.0",
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/production/index.js",
  "module": "dist/esm/production/index.js",
  "exports": {
    ".": {
      "development": {
        "require": "./dist/cjs/development/index.js",
        "import": "./dist/esm/development/index.js"
      },
      "require": "./dist/cjs/production/index.js",
      "import": "./dist/esm/production/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "env.d.ts"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "devDependencies": {
    "@types/mdast": "^3.0.10",
    "@types/node": "^17.0.35",
    "eslint": "^8.19.0",
    "eslint-config-lxsmnsyc": "^0.4.7",
    "pridepack": "2.0.1",
    "solid-js": "^1.4.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vitest": "^0.18.1"
  },
  "peerDependencies": {
    "solid-js": "^1.2"
  },
  "dependencies": {
    "mdast-util-from-markdown": "^1.2.0",
    "mdast-util-gfm": "^2.0.1",
    "mdast-util-mdx": "^2.0.0",
    "micromark-extension-gfm": "^2.0.1",
    "micromark-extension-mdxjs": "^1.0.0",
    "source-map": "^0.7.4"
  },
  "description": "Markdown Compiler for SolidJS",
  "repository": {
    "url": "https://github.com/lxsmnsyc/solid-marked.git",
    "type": "git"
  },
  "homepage": "https://github.com/lxsmnsyc/solid-marked/tree/main/packages/solid-marked",
  "bugs": {
    "url": "https://github.com/lxsmnsyc/solid-marked/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexis Munsayac",
  "private": false,
  "scripts": {
    "prepublishOnly": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "pridepack check",
    "lint": "pridepack lint",
    "test": "vitest",
    "clean": "pridepack clean",
    "watch": "pridepack watch",
    "start": "pridepack start",
    "dev": "pridepack dev"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/index.d.ts"
      ]
    }
  }
